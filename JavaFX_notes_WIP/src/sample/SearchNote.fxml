<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1"  fx:controller="sample.ViewNoteController">
    <children>
        <Pane fx:id="pane" prefHeight="500" prefWidth="700">
            <children>
                <Button fx:id="searchButton" layoutX="200.0" layoutY="10.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Search note" />
                <TextArea fx:id="searchText" layoutX="110.0" layoutY="100.0" prefHeight="50.0" prefWidth="100.0" />
                <TextArea fx:id="text" layoutX="210.0" layoutY="100.0" prefHeight="50.0" prefWidth="500.0" />
            </children>
        </Pane>
    </children>
</GridPane>