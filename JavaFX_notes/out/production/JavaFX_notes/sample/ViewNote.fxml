<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="sample.ViewNoteController">
    <children>
        <Pane fx:id="pane" prefHeight="500" prefWidth="700">
            <children>
                <Button fx:id="removeButton" layoutX="125.0" layoutY="10.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Remove this note" />
                <Button fx:id="saveButton" layoutX="427.0" layoutY="10.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Save changes" />
                <Button fx:id="editButton" layoutX="276.0" layoutY="10.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Edit this note" />
                <TextArea fx:id="number" layoutX="10.0" layoutY="100.0" prefHeight="50.0" prefWidth="100.0" />
                <TextArea fx:id="name" layoutX="110.0" layoutY="100.0" prefHeight="50.0" prefWidth="100.0" />
                <TextArea fx:id="text" layoutX="210.0" layoutY="100.0" prefHeight="50.0" prefWidth="500.0" />
                <TextArea fx:id="numberColumn" layoutX="10.0" layoutY="150.0" prefHeight="300.0" prefWidth="100.0" />
                <TextArea fx:id="nameColumn" layoutX="110.0" layoutY="150.0" prefHeight="300.0" prefWidth="100.0" />
                <TextArea fx:id="textColumn" layoutX="210.0" layoutY="150.0" prefHeight="300.0" prefWidth="500.0" />
            </children>
        </Pane>
    </children>
</GridPane>
