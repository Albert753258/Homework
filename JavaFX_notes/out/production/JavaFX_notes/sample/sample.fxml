<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="sample.Controller">
    <children>
        <Pane fx:id="pane" prefHeight="500" prefWidth="700">
            <children>
                <Button fx:id="addButton" layoutX="400.0" layoutY="105.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="Add new note" />
                <Button fx:id="viewButton" layoutX="450.0" layoutY="155.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="150.0" text="View note" />
                <TextField fx:id="numberOfView" layoutX="400.0" layoutY="155.0" prefHeight="50.0" prefWidth="50.0" />
                <TextArea fx:id="text" layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="350.0" />
            </children>
        </Pane>
    </children>
</GridPane>
